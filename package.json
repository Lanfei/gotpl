  },
  "keywords": [
    "template",
    "tpl"
  ],
  "dependencies": {
    "escape-html": "^1.0.3",
    "is-keyword-js": "^1.0.3",
    "js-tokens": "^4.0.0"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "expect.js": "^0.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "uglify-js": "^3.4.9"
  }
